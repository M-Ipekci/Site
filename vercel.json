{
    "builds": [
      {
        "src": "./src/App.js",
        "use": "@vercel/node",
        "config": { "maxLambdaSize": "15mb" }
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/.*", "dest": "/index.html" }
    ]
  }
